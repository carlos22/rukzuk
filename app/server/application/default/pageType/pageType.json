{
  "name": {
    "de": "Normale Seite",
    "en": "Regular Page"
  },
  "description": {
    "de": "Standard für normale Seiten.",
    "en": "Default for regular pages."
  },
  "version": "dev",
  "form": [
    {
      "type": "TextField",
      "CMSvar": "_name",
      "fieldLabel": "{\"de\": \"Seitentitel\", \"en\": \"Page Title\"}",
      "emptyText": "",
      "minLength": 1,
      "maxLength": 255,
      "allowBlank": true
    },
    {
      "type": "OnOffFieldSet",
      "CMSvar": "_inNavigation",
      "fieldLabel": "{\"de\": \"In Navigation anzeigen\", \"en\": \"Show in Navigation\"}",
      "_items": [
        {
          "type": "TextField",
          "CMSvar": "_navigationTitle",
          "fieldLabel": "{\"de\": \"Eigene Bezeichnung in Navigation (Standard: Seitentitel)\", \"en\": \"Name in Navigation (default: Page Title)\"}",
          "emptyText": "",
          "minLength": 0,
          "maxLength": 255,
          "allowBlank": true
        }
      ]
    },
    {
      "type": "TextArea",
      "CMSvar": "_description",
      "fieldLabel": "{\"de\": \"Beschreibung für Teaser und Suchmaschinen\", \"en\": \"Description for Teaser and Search Engines\"}",
      "height": 100
    },
    {
      "type": "DateField",
      "CMSvar": "_date",
      "width": 200,
      "fieldLabel": "{\"de\": \"Datum\", \"en\": \"Date\"}"
    },
    {
      "type": "ImagePicker",
      "CMSvar": "_mediaId",
      "filterType":"image",
      "showText": true,
      "fieldLabel": "{\"de\": \"Bild\", \"en\": \"Image\"}",
      "text": "{\"de\": \"Bild wählen\", \"en\": \"Select image\"}",
      "multiSelect": false
    },
    {
      "type": "OnOffToggle",
      "CMSvar": "redirectToFirstChild",
      "fieldLabel": "{\"de\": \"Auf die erste darunterliegende Seite weiterleiten\", \"en\": \"Redirect to first child page\"}"
    },
    {
      "type": "OnOffToggle",
      "CMSvar": "notInPageList",
      "fieldLabel": "{\"de\": \"Diese Seite nicht in Seitenlisten anzeigen\", \"en\": \"Hide this page on page lists\"}"
    }
  ],
  "formValues": {
    "_name": "",
    "_inNavigation": true,
    "_navigationTitle": "",
    "_date": null,
    "_description": "",
    "_mediaId": null
  }
}